{
  "name": "frontendmedicalrecord",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:report": "eslint . --format json --output-file reports/eslint-report.json || eslint . --format html --output-file reports/eslint-report.html",
    "audit:report": "npm audit --json > reports/npm-audit.json || true && node generate-audit-html.cjs",
    "audit:dependencies": "npm audit --audit-level=moderate",
    "audit:runtime": "./owasp-zap-scan.sh",
    "security:check": "npm run lint && npm audit --audit-level=moderate",
    "security:full": "npm run lint:report && npm run audit:report && npm run audit:runtime",
    "security:dashboard": "./open-security-dashboard.sh",
    "jenkins:trigger": "node scripts/jenkins-trigger-and-capture.js",
    "preview": "vite preview",
    "serve:ci": "vite preview --host 0.0.0.0 --port 4173 --strictPort",
    "test:selenium": "node tests/selenium.smoke.js",
    "test": "vitest",
    "test:unit": "vitest --run --pool=threads --test-timeout=60000 --hook-timeout=60000",
    "test:watch": "vitest",
    "coverage": "vitest --run --coverage",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "eslint --format json --output-file reports/eslint-report.json"
    ]
  },
  "dependencies": {
    "@unleash/proxy-client-react": "^5.0.1",
    "frontendmedicalrecord": "file:",
    "jwt-decode": "^4.0.0",
    "launchdarkly-react-client-sdk": "^3.0.0",
    "lucide-react": "^0.464.0",
    "react": "^18.3.1",
    "react-datepicker": "^7.5.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "react-time-picker": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.11",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^4.0.3",
    "autoprefixer": "^10.4.20",
    "chromedriver": "^141.0.4",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^15.5.2",
    "postcss": "^8.4.47",
    "react-day-picker": "^9.3.2",
    "selenium-webdriver": "^4.38.0",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.3"
  }
}
